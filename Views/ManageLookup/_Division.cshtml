<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/css/site.css" asp-append-version="true" />
</head>

<br />
<div class="card mb-sm-3" style="min-width: 18rem;">
    <h4 class="card-header" id="Division">Division</h4>
    <div class="card-body">
        @Html.DropDownList("DivisionID", null, htmlAttributes: new { @size = 6, @class = "form-control" })
    </div>
    <div class="card-footer">
        <a asp-controller="Division" asp-action="Create">Create</a> |
        <a asp-controller="Division" asp-action="Edit" class="linkClick">Edit</a> |
        <a asp-controller="Division" asp-action="Delete" class="linkClick">Delete</a>
    </div>
</div>